leetcode
###  231. Power of Two
class Solution {
public:
    bool isPowerOfTwo(int num) {
        if(((num & (num-1))==0) && num)
            return true;
        
        return false;
        
    }
};


### 342. Power of Four
class Solution {
public:
    bool isPowerOfFour(int num) {
        if ((num &(num-1))==0)
            if(num & 0x55555555)
                return true;
        return false;
    }
};
